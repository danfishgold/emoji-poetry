{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client", "jest"],
    "paths": {
      "cmudict": ["./types/cmudict.d.ts"],
      "unicode/category/So": ["./types/unicode.d.ts"],
      "emojiData": ["./src/emojiData.json"]
    },
    "allowJs": false,
    "checkJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": ["./src", "generateEmoji.ts", "./jestSetup.ts"],
  "exclude": ["./types"],
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  }
}
