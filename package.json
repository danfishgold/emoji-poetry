{
  "name": "emoji-poems",
  "scripts": {
    "build:emoji": "ts-node --project ./tsconfig.json src/generateEmoji.ts > src/emoji.json",
    "build:src": "tsc && vite build",
    "dev:src": "vite",
    "dev": "npm-run-all -s build:emoji dev:src",
    "build": "npm-run-all build:emoji build:src",
    "serve": "vite preview"
  },
  "dependencies": {
    "cmudict": "^1.0.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "unicode": "^13.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "vite": "^2.0.0-beta.70"
  }
}
