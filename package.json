{
  "name": "emoji-poems",
  "scripts": {
    "build:emoji": "ts-node --project ./tsconfig.json src/generateEmoji.ts > src/emojiData.json",
    "build:src": "tsc && vite build",
    "dev:src": "vite",
    "dev": "npm-run-all -s build:emoji dev:src",
    "build": "npm-run-all -s build:emoji build:src",
    "serve": "vite preview",
    "test": "jest --watch"
  },
  "dependencies": {
    "cmudict": "^1.0.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "unicode": "^13.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.1.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "vite": "^2.0.0-beta.70"
  }
}
